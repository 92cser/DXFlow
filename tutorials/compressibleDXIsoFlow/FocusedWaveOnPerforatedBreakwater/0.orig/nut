FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    location    "0";
    object      nut;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 2 -1 0 0 0 0];


internalField   uniform 0;

boundaryField
{
    inlet
    {
        type            nutkWallFunction;
        value           uniform 0;
    }
    outlet
    {
        type            nutkWallFunction;
        value           uniform 0;
    } 
    bottom
    {
        type            nutkWallFunction;
        value           uniform 0;
    }   
    slope
    {
        type            nutkWallFunction;
        value           uniform 0;
    } 
    PCB
    {
        type            nutkWallFunction;
        value           uniform 0;
    }     
    atmosphere
    {
        type            calculated;
        value           uniform 0;
    }
    frontAndBack
    {
        type            empty;
    }
}


